{
  "name": "vademecumas/yii2-auth",
  "description": "Auth extension for yii2 projects",
  "autoload": {
    "psr-4": {
      "vademecumas\\auth\\": "src"
    }
  },
  "authors": [
    {
      "name": "dev",
      "email": "dev@vademecumonline.com.tr"
    }
  ],
  "require": {
    "php": ">=7.3",
    "yiisoft/yii2": "~2.0.14",
    "yiisoft/yii2-bootstrap4": "~2.0.0",
    "hail812/yii2-adminlte3": "~1.1",
    "himiklab/yii2-recaptcha-widget": "^2.1"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "config": {
    "allow-plugins": {
      "yiisoft/yii2-composer": true
    }
  }
}
